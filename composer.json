{
	"name": "froozeify/sqlite-backup-bundle",
	"description": "Easy way to backup/restore an SQLite file in a symfony project",
	"type": "symfony-bundle",
	"license": "MIT",
	"authors": [
		{
			"name": "Froozeify",
			"email": "froozeify@mail.com"
		}
	],
	"config": {
		"sort-packages": true
	},
	"require": {
		"php": "^7.2",
		"psr/container": "^1.0",
		"symfony/config": "^4.2",
		"symfony/console": "^4.2",
		"symfony/dependency-injection": "^4.2",
		"symfony/http-kernel": "^4.2"
	},
	"require-dev": {
	},
	"autoload": {
		"psr-4": {
			"Froozeify\\SqliteBackupBundle\\": ""
		},
		"exclude-from-classmap": [
			"/Tests/"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Froozeify\\SqliteBackupBundle\\Tests\\": "Tests"
		}
	}
}
